{
  "name": "student-accountability-app",
  "version": "1.0.0",
  "description": "MERN stack student accountability application",
  "scripts": {
    "install:all": "cd server && pnpm install && cd ../client && pnpm install",
    "dev:server": "cd server && pnpm dev",
    "dev:client": "cd client && pnpm dev",
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "start:server": "cd server && pnpm start",
    "build:client": "cd client && pnpm build"
  },
  "keywords": ["mern", "student", "accountability", "education"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.2"
  }
}
